<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.vicmakmanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/vicmakicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/vicmakicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.VicmakManager"
        tools:targetApi="31">
        <activity
            android:name=".commissions.CommissionsDisplay"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".users.ViewUsers"
            android:exported="false" />
        <activity
            android:name=".ViewOrders"
            android:exported="false" />
        <activity
            android:name=".RefillingGas"
            android:exported="false" />
        <activity
            android:name=".ChangeCredentials"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".Burners.UpdateBurners"
            android:exported="false" />
        <activity
            android:name=".Grills.UpdateGrills"
            android:exported="false" />
        <activity
            android:name=".Pipes.UpdatePipes"
            android:exported="false" />
        <activity
            android:name=".Regulators.UpdateRegulators"
            android:exported="false" />
        <activity
            android:name=".Regulators.RegulatorsManagement"
            android:exported="false" />
        <activity
            android:name=".Gas.UpdateGas"
            android:exported="false" />
        <activity
            android:name=".Pipes.PipesManagement"
            android:exported="false" />
        <activity
            android:name=".Grills.GrillsManagement"
            android:exported="false" />
        <activity
            android:name=".Burners.BurnerManagement"
            android:exported="false" />
        <activity
            android:name=".Gas.GasManagement"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".NotifyOrders"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>